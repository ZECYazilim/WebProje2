@model IEnumerable<ApiSon>
@{
    Layout = null;
}
<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.USD</td>
                <td>@r.degisim</td>
            </tr>
        }
    </tbody>
</table>